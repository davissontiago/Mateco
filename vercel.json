{
  "version": 2,
  "buildCommand": "python manage.py collectstatic --noinput",
  "builds": [
    {
      "src": "setup/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/(.*)",
      "dest": "setup/wsgi.py"
    }
  ]
}
