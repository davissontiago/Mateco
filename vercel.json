{
    "version": 2,
    "builds": [
        {
            "src": "setup/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxDuration": 10,
                "includeFiles": "staticfiles/**"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "setup/wsgi.py"
        }
    ]
}